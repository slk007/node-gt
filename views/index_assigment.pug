extends layout

block content
  .container
    h1 Assigment List
    table
      tr
        th Title
        th Due Date
        th Option
      if data
        each file in data
          tr
            td #{file.title}
            td #{file.dueDate}
            td
               a(class='button', href=`download/assigment/${file.key}/${file.originalname}` id='download' ) Download
      else
        p No Assigments Yet
